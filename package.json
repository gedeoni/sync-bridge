{
  "name": "exporter-service",
  "version": "1.1.8",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "npm run build && node build/server.js",
    "build": "npm run build:clean && tsc",
    "build:clean": "rm -f -R build",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "db:sync": "ts-node src/databases/sync",
    "db:drop": "ts-node src/databases/drop",
    "db:migrate": "sequelize-cli db:migrate --config src/databases/config/config.json --migrations-path migrations",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --config src/databases/config/config.json --migrations-path migrations",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all --config src/databases/config/config.json --migrations-path migrations",
    "db:migrate:status": "sequelize-cli db:migrate:status --config src/databases/config/config.json --migrations-path migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^10.0.0",
    "bunyan": "^1.8.15",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.3",
    "sequelize-typescript": "^2.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.3",
    "@types/pg": "^8.10.9",
    "@types/sequelize": "^4.28.19",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^29.7.0",
    "lint-staged": "^12.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.0",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "volta": {
    "node": "20.18.1"
  }
}
